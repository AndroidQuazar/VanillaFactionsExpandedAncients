<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="SkyfallerBase">
    <defName>VFEA_SupplyCrateIncoming</defName>
    <label>supply crate (incoming)</label>
    <size>(2, 2)</size>
    <graphicData>
      <texPath>Things/Building/Misc/AncientSupplyCrate/AncientSupplyCrateSkyfaller</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(5, 5)</drawSize>
    </graphicData>
    <skyfaller>
      <shadowSize>(3.0, 3.0)</shadowSize>
      <ticksToImpactRange>240~300</ticksToImpactRange>
      <speed>0.05</speed>
      <movementType>Decelerate</movementType>
      <motesPerCell>1</motesPerCell>
    </skyfaller>
    <comps>
      <li Class="CompProperties_SkyfallerRandomizeDirection">
        <directionChangeInterval>30~60</directionChangeInterval>
        <maxDeviationFromStartingAngle>15</maxDeviationFromStartingAngle>
      </li>
    </comps>
  </ThingDef>
</Defs>
