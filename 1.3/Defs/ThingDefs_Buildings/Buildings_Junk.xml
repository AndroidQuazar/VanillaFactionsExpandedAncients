  <ThingDef ParentName="BuildingBase">
    <defName>VFEA_AncientFuelTank</defName>
    <label>ancient fuel tank</label>
    <description>A large fuel tank that can be deconstructed for fuel.</description>
    <size>(2, 3)</size>
    <fillPercent>0.50</fillPercent>
    <graphicData>
      <texPath>Things/Building/Junk/AncientFuelTank/AncientFuelTank</texPath>
