<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <WorkGiverDef>
        <defName>VFEA_DoBillsAncientWorkshop</defName>
        <label>make things at ancient workshop</label>
        <giverClass>WorkGiver_DoBill</giverClass>
        <workType>Smithing</workType>
        <priorityInType>50</priorityInType>
        <fixedBillGiverDefs>
            <li>VFEA_AncientWorkshop</li>
        </fixedBillGiverDefs>
        <verb>work</verb>
        <gerund>working at</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <prioritizeSustains>true</prioritizeSustains>
    </WorkGiverDef>
	
